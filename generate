  <% end %>
  <% form_for :image, @image, :url => url_path, :html => { :method => :post, :multipart => true } do |f| %>
    <%= f.label :description, "Description:" %>
    <%= f.text_field :description %><br/>

    <div id="image_variants">
      <%= render :partial => 'image_variants/image_variants', :collection => @image.image_variants %>
    </div>
 
    <%= link_to_function "Add Image Variant" do |page|
      page.insert_html :bottom, :image_variants, :partial => 'image_variants/_new', :object => ImageVariant.new
    end %>

    <p><%= f.submit "Create" %></p>
  <% end %>
</div>
