7[?47h[?1h=[32;1H[Kclass CollectionController < ApplicationController

  layout "archive"

  def show
    @archive = session['current_archive']
    @collection = Collection.find( params[:id] )
    session['current_collection'] = @collection.id
  end

  def edit
    @archive = session['current_archive']
    @collection = Collection.find( params[:id] )
    session['current_collection'] = @collection.id
  end

  def create_image
    @archive = session['current_archive']
    @collection = Collection.find( params[:id] )
    session['current_collection'] = @collection.id
  end

  def add_image
    @archive = session['current_archive']
    @collection = Collection.find( params[:id] )
    @new_image = Image.create( params[:image] )
    @new_image.archive_id = @archive.id
    @new_image.collection_ids =  [ params[:id] ]
    @new_image.save
    session['current_collection'] = @collection.id

