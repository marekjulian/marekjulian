7[?47h[?1h=[32;1H[Kclass ImageController < ApplicationController

  layout "archive"

  def show
    @archive = session['current_archive']
    @collection = Collection.find( session['current_collection'] )
    @image = Image.find( params[:id] )
  end

  def edit
    @archive = session['current_archive']
    @collection = Collection.find( session['current_collection'] )
    @image = Image.find( params[:id] )
  end

  def create_image_variant
    @archive = session['current_archive']
    @collection = Collection.find( session['current_collection'] )
    @image = Image.find( params[:id] )
  end

  def add_image_variant
    @archive = session['current_archive']
    @collection = Collection.find( session['current_collection'] )
    @image = Image.find( params[:id] )
    @show_string = params

    @new_variant = ImageVariant.create( params[:image_variant] )
    @new_variant.save

