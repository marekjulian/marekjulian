  def [7mdestroy[m
    @collection = Cm::Collection.find(params[:id])
    @collection.[7mdestroy[m

    respond_to do |format|
      format.html { redirect_to(cm_collections_url) }
      format.xml  { head :ok }
    end
  end
end
