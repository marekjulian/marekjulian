<% if images %>
    <% images.each do | image | %>
        <% image_tag_id = "organizer-preview-image-#{tab_id}-tab-#{image.id}" %>
        <%= organizer_image_tag image, image_tag_id, "org-prev-img" %>
        <%= draggable_element "#{image_tag_id}", :ghosting => true, :revert => true %>
   <% end %>
<% end %>
